@model IEnumerable<AuthSystem.Models.OdlFaseVersamento>

@{
    ViewData["Title"] = "Index";
}

<h1>Versamento</h1>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Articoli)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Odl)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OdlFasi)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MacchinaFisiche)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AspNetUsers)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DataInizio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DataFine)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TempoLavoroNetto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PezziBuoni)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PezziScartati)
            </th>
            <th><a asp-action="Create" class="btn btn-outline-success "><i class="far fa-plus-square"></i>Versamento</a></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Articoli.CodiceArticolo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Odl.CodiceOdl)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OdlFasi.IdFaseOdl)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MacchinaFisiche.CodiceMacchinaFisica)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AspNetUsers.UserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DataInizio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DataFine)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TempoLavoroNetto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PezziBuoni)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PezziScartati)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdVersamento">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdVersamento">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdVersamento">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
